clc; 
clear; 
k=10; 
s = %s;
num = 1;
den = s^3 + s + 0.6*s^2 + 0.05;
s1 = syslin('c', num, den);
 
G=k*s1; 
disp(G, 'The given transfer function G(s)='); 
bode(G,0.01,1000); 
xtitle('Bode plot of the given transfer function G(s)'); 
[g,frp]=g_margin(G); 
[p,frg]=p_margin(G) 
show_margins(G) 
disp(frg,'Gain crossover frequency=',p,'Phase margin(degrees)= '
